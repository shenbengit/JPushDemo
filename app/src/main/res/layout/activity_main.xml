<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical"
    tools:context="com.example.ben.jpushdemo.activity.MainActivity">

    <Button
        android:id="@+id/btn_init"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="init" />

    <Button
        android:id="@+id/btn_stop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="stop" />

    <Button
        android:id="@+id/btn_resume"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="resume" />

    <Button
        android:id="@+id/btn_isStopped"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="isPushStopped" />

    <Button
        android:id="@+id/btn_registrationId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="registration_id" />

    <TextView
        android:id="@+id/tv_registrationId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="RegistrationId:" />

    <Button
        android:id="@+id/btn_jump"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="跳转" />
</LinearLayout>
